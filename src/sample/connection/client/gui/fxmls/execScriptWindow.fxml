<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.connection.client.gui.ExecScriptWindow">
   <children>
      <TextArea fx:id="executionRes" layoutX="14.0" layoutY="94.0" prefHeight="292.0" prefWidth="576.0" />
      <Button fx:id="chooseFileButton" layoutX="471.0" layoutY="35.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="115.0" text="Choose file" />
      <Text fx:id="cmdName" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Execute script" textAlignment="CENTER" wrappingWidth="600.0">
         <font>
            <Font size="20.0" />
         </font>
      </Text>
      <HBox layoutX="19.0" layoutY="48.0" prefHeight="19.0" prefWidth="409.0">
         <children>
            <Text fx:id="chosenFileText" strokeType="OUTSIDE" strokeWidth="0.0" text="Chosen file: ">
               <font>
                  <Font size="14.0" />
               </font>
            </Text>
            <Text fx:id="filename" strokeType="OUTSIDE" strokeWidth="0.0">
               <font>
                  <Font size="14.0" />
               </font>
            </Text>
         </children>
      </HBox>
      <Text fx:id="resultText" layoutX="19.0" layoutY="90.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Result of execution in console:" />
      <Button fx:id="stopScriptButton" layoutX="471.0" layoutY="65.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="115.0" text="Stop script" />
      <ProgressIndicator fx:id="progressBar" layoutX="276.0" layoutY="174.0" visible="false" />
   </children>
</AnchorPane>
